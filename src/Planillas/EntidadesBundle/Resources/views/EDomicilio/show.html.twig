{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EDomicilio</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Direccionexacta</th>
                <td>{{ entity.direccionExacta }}</td>
            </tr>
            <tr>
                <th>Canton</th>
                <td>{{ entity.canton }}</td>
            </tr>
            <tr>
                <th>Tiemporesidencia</th>
                <td>{{ entity.tiempoResidencia }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('domicilio') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('domicilio_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
