{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EComponentesSalariales list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Componente</th>
                <th>Cantidad</th>
                <th>Moneda</th>
                <th>Descripcion</th>
                <th>Fechavencimiento</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('ecomponentessalariales_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.componente }}</td>
                <td>{{ entity.cantidad }}</td>
                <td>{{ entity.moneda }}</td>
                <td>{{ entity.descripcion }}</td>
                <td>{% if entity.fechaVencimiento %}{{ entity.fechaVencimiento|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('ecomponentessalariales_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ecomponentessalariales_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('ecomponentessalariales_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
