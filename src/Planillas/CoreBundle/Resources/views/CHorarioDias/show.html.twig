{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CHorarioDias</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Dia</th>
                <td>{{ entity.dia }}</td>
            </tr>
            <tr>
                <th>Horainicio</th>
