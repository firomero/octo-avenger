<form id="turno-form" action="" method="post" {{ form_enctype(form) }} style="padding-top: 8px; padding-bottom: 8px;">
    <div class="row">
        <div class="col-lg-4">{{ form_row(form.empresa) }}</div>
        <div class="col-lg-4">{{ form_row(form.cliente) }}</div>
        <div class="col-lg-4">{{ form_row(form.sucursal) }}</div>
    </div>
    <div class="row">
        <div class="col-lg-4">{{ form_row(form.nombre) }}</div>
    </div>
    <div><a class="btn btn-primary submit-button" href="#" data-content="turno"><i class="glyphicon-plus"></i> Adicionar</a></div>
</form>
