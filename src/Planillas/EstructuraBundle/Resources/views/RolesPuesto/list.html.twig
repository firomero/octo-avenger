<table class="table table-striped table-bordered table-condensed table-responsive">
    <thead>
    <tr>
        <th>Nombre Puesto</th>
        <th>Rol</th>
        <th>Acciones</th>
    </tr>
    </thead>
    <tbody>
    {% for e in entities %}
        <tr>
            <td>{{ e.puesto }}</td>
            <td>{{ e.rol }}</td>
            <td></td>
        </tr>
    {% else %}
        <tr><td colspan="3">No se encontraron elementos.</td></tr>
    {% endfor %}
    </tbody>
</table>


<div style="text-align: center;">{{ knp_pagination_render(entities) }}</div>